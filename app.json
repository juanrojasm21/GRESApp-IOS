{
  "expo": {
    "name": "GRESapp",
    "slug": "GRESapp",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.caeptudea.gresappios",
      "supportsTablet": true
    },
    "android": {
      "package": "com.caeptudea.gresapp",
      "versionCode": 1
    }
  }
}
